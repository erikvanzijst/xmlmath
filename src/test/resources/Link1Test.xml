<?xml version="1.0" encoding="UTF-8"?>
<test xmlns="http://xmlmath.org/1.0">
	
	<testBoolean result="true">
		<expression>
			<and>
				<declare name="x">
					<nil/>
				</declare>
				<declare name="y">
					<long value="2"/>
				</declare>
				<declare name="z">
					<double value="3.0"/>
				</declare>
				<declare name="q">
					<string value="foobar"/>
				</declare>
	
				<equals>
					<nil/>
					<link name="x"/>
				</equals>
	
				<equals>
					<linkLong name="y"/>
					<long value="2"/>
				</equals>
	
				<equals>
					<linkDouble name="z"/>
					<double value="3.0"/>
				</equals>
	
				<equals>
					<linkString name="q"/>
					<string value="foobar"/>
				</equals>
	
			</and>
		</expression>
	</testBoolean>
</test>
